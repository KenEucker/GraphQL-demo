{
  "name": "graphql-demo",
  "version": "0.0.1",
  "type": "module",
  "license": "AGPL-3.0-or-later",
  "prisma": {
    "schema": "src/schema/schema.prisma"
  },
  "scripts": {
    "analyze": "windicss-analysis",    
    "postinstall": "npm run compile",
    "build": "npm run build:client",
    "build:client": "vue-tsc && vite build",
    "build:prisma": "prisma db push",
    "build:prod": "prisma generate",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\" \"npm run start:prisma\" -n \"server, client, studio\" -c \"auto\"",
    "start:client": "vite preview --host",
    "start:server": "ts-node --experimental-specifier-resolution=node --project tsconfig-server.json server.ts",
    "start:prisma": "prisma studio",
    "deploy:prisma": "prisma migrate dev",
    "release": "npm run release:prisma",
    "release:prod": "npm run release:prisma",
    "release:prisma": "prisma migrate deploy",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"  \"graphql-codegen -w\" \"npm run start:prisma\" -n \"server, client, codegen, prisma\" -c \"auto\"",
    "dev:server": "nodemon --watch './src/schema/**/*.ts' --exec 'npm run start:server'",
    "dev:client": "vite",
    "compile": "npm run compile:prisma && npm run compile:graphql",
    "compile:graphql": "graphql-codegen",
    "compile:prisma": "prisma generate",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint --ext .ts,.js,.vue src --fix",
    "lint:style": "stylelint \"./src/**/*.vue\" \"./src/**/*.scss\" --fix"
  },
  "dependencies": {
    "@apollo/client": "^3.7.2",
    "@vue/apollo-composable": "^4.0.0-beta.1",
    "@vueuse/components": "^9.6.0",
    "@vueuse/core": "^9.6.0",
    "@vueuse/motion": "^2.0.0-beta.12",
    "concurrently": "^7.6.0",
    "prisma": "^4.7.1",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.28",
    "uuid": "^9.0.0",
    "ts-node": "^10.9.1",
    "vite": "^4.0.0",
    "vue": "^3.2.45",
    "vue-ionicons": "^3.0.5",
    "vue-router": "^4.1.6",
    "@apollo/server": "^4.3.0",
    "@graphql-codegen/add": "^3.2.3",
    "@graphql-codegen/cli": "2.16.1",
    "@graphql-codegen/client-preset": "1.2.3",
    "@graphql-codegen/introspection": "^2.2.3",
    "@graphql-codegen/named-operations-object": "^2.3.1",
    "@graphql-codegen/near-operation-file-preset": "^2.4.4",
    "@graphql-codegen/typed-document-node": "^2.3.10",
    "@graphql-codegen/typescript": "^2.8.5",
    "@graphql-codegen/typescript-apollo-client-helpers": "^2.2.6",
    "@graphql-codegen/typescript-operations": "^2.5.10",
    "@graphql-codegen/typescript-resolvers": "^2.7.10",
    "@graphql-codegen/typescript-vue-apollo": "^3.3.6",
    "@prisma/studio": "^0.479.0",
    "@types/nprogress": "^0.2.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "graphql-yoga": "^3.1.1",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.20",
    "postcss-html": "^1.5.0",
    "prisma-class-validator-generator": "^0.2.0",
    "sass": "^1.56.2",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-order": "^5.0.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.3",
    "vite-plugin-windicss": "^1.8.10",
    "vue-tsc": "^1.0.11",
    "windicss": "^3.5.6",
    "windicss-analysis": "^0.3.5"
  }
}
